        if (intervaloMedicamento == '') {
            alert("Por favor, insira o intervalo de medicação.");
            return;
        }

        var novaDiv = $('</br> <div class="col-md-11 row" id="idMedicamentoContainer' + contadorMedicamento + '" style="padding-top: 1rem;"> <div class="col - md - 3">  <input readonly name="Medicamentos' + contadorMedicamento + '" class="form - control col - md - 3" value="' + nomeMedicamento + '" /></div> <div class="col - md - 3">  <input readonly name="Medicamentos' + contadorMedicamento + '" class="form - control col - md - 3" value="' + dosagemMedicamento + '" /></div>  <div class="col - md - 3">  <input readonly name="Medicamentos' + contadorMedicamento + '" class="form - control col - md - 3" value="' + intervaloMedicamento + '" /></div> <div class="col - md - 3">  <input readonly name="Medicamentos' + contadorMedicamento + '" class="form - control col - md - 3" value="' + duracaoMedicamento + '" /></div>   </div> <div class="col - md - 1" style="padding-top: 0.15rem; " id="idMedicamentoBtn' + contadorMedicamento + '"> <button type="button" onclick="RemoverMedicamento(' + contadorMedicamento + ', true)" class="btn btn-sm btn-primary">-</button> </div>');


        $('<input>').attr({
            type: 'hidden',
            name: 'Prescricao.Medicamentos[' + contadorMedicamento + '].Medicacao',
            value: nomeMedicamento
        }).appendTo('#divMedicamentosAdicionados');

        $('<input>').attr({
            type: 'hidden',
            name: 'Prescricao.Medicamentos[' + contadorMedicamento + '].Dosagem',
