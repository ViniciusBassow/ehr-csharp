            $("#divDocumentos").show();
            $("#divPrincipal").hide();
            $("#divConsulta").hide();

            $("#navHemograma").removeClass("active");
            $("#navDocumentos").addClass("active");
            $("#navPrincipal").removeClass("active");
            $("#navConsulta").removeClass("active");
        }
        else if (aba == 'Consulta') {
            $("#divHemograma").hide();
            $("#divDocumentos").hide();
            $("#divPrincipal").hide();
            $("#divConsulta").show();

            $("#navHemograma").removeClass("active");
