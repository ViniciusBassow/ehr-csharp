<script>
    $(document).ready(function () {
        if ("@Model.Editar" != 'True') {
            $("#formSalvarPaciente :input").prop("disabled", true);
        }
    });

    document.addEventListener("DOMContentLoaded", function () {
        const cidInput = document.querySelector("#Cid");

        cidInput.addEventListener("input", function () {
            let value = cidInput.value.toUpperCase(); // Converte para maiúsculas
            value = value.replace(/[^A-Z0-9.]/g, ""); // Remove caracteres inválidos (apenas letras, números e ponto)

            // Enforce formato: letra + 2 números + (opcional: ponto + 1-2 números)
            const regex = /^([A-Z])(\d{0,2})(\.?)(\d{0,2})$/;
            const match = value.match(regex);

            if (match) {
                let formattedValue = match[1]; // Letra
                if (match[2]) formattedValue += match[2]; // Números
                if (match[3]) formattedValue += "."; // Ponto
                if (match[4]) formattedValue += match[4]; // Mais números
                cidInput.value = formattedValue;
            } else {
                cidInput.value = value.slice(0, -1); // Remove último caractere inválido
            }
        });
    });

    let contador = 0;
    function AdicionarAntecedente() {
        var inputValue = $('#AntecedenteInput').val();

        if (inputValue.trim() === "") {
            alert("Por favor, insira um valor.");
            return;
        }


        var novaDiv = $('<div class="col-md-11" id="idAntecedenteContainer' + contador + '"> <label class= "form-label" for="AntecedenteDesc"> </label> <input readonly name="Antecedentes' + contador + '" class="form-control" value="' + inputValue + '" /> </div><div class="col-md-2" style="padding-top: 0.5rem;" id="idAntecedenteBtn' + contador + '"> <button type="button" onclick="RemoverAntecedente(' + contador + ')" class="btn btn-sm btn-primary">Remover</button> </div>');


        $('<input>').attr({
            type: 'hidden',
            name: 'Antecedentes[' + contador + '].Descricao',
            value: inputValue
        }).appendTo('#divContainer');

        $('#divContainer').append(novaDiv);
        contador++;
        $('#AntecedenteInput').val('');
    }

    function RemoverAntecedente(id) {
        $("#idAntecedenteContainer" + id).remove();
        $("#idAntecedenteBtn" + id).remove();
        $("input[name='Antecedentes[" + id + "].Descricao']").remove();
    }


    let contadorMedicamento = @(Model.ultimaConsulta?.Prescricao?.Medicamentos != null
        ? Model.ultimaConsulta.Prescricao.Medicamentos.Count()
        : 0);

    function AdicionarMedicamento() {
        var nomeMedicamento = $('#MedicamentoInput').val();
        var dosagemMedicamento = $('#DosagemInput').val();
        var intervaloMedicamento = $('#IntervaloInput').val();
        var duracaoMedicamento = $('#DuracaoInput').val();

        if (nomeMedicamento == '') {
            alert("Por favor, insira o medicamento.");
            return;
