        }
        else if (aba == 'Hemograma') {
            $("#divHemograma").show();
            $("#divDocumentos").hide();
            $("#divPrincipal").hide();
            $("#divConsulta").hide();

            $("#navHemograma").addClass("active");
            $("#navDocumentos").removeClass("active");
            $("#navPrincipal").removeClass("active");
            $("#navConsulta").removeClass("active");
        }
        else if (aba == 'Documentos') {
