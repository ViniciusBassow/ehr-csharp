    function RemoverMedicamento(id, novo) {
        if (novo) {
            $("#idMedicamentoContainer" + id).remove();
            $("#idMedicamentoBtn" + id).remove();

            $("input[name='Medicamento[" + id + "].Medicacao']").remove();
            $("input[name='Medicamento[" + id + "].Dosagem']").remove();
            $("input[name='Medicamento[" + id + "].Intervalo']").remove();
            $("input[name='Medicamento[" + id + "].Duracao']").remove();
        }
        else {
            if (!confirm("Tem certeza de que deseja remover esta Medicação? \nEsta ação não poderá ser desfeita!")) return;

            var medicamentoId = $("#Medicamento"+id+"Id").val();

            $.ajax({
                url: '@Url.Action("ExcluirMedicamentoPrescricao", "Consulta")',
                type: 'POST',
                data: { "Id": medicamentoId },
                success: function (data) {
                    $("#idMedicamentoContainer" + id).remove();
                    $("#idMedicamentoBtn" + id).remove();

                    $("input[name='Medicamento[" + id + "].Medicacao']").remove();
                    $("input[name='Medicamento[" + id + "].Dosagem']").remove();
                    $("input[name='Medicamento[" + id + "].Intervalo']").remove();
                    $("input[name='Medicamento[" + id + "].Duracao']").remove();

                    displayMensagemSucessoAjax();
                },
                error: function (xhr, status, error) {
                    alert("Ocorreu um erro ao excluir prescrição");
                }
            });
        }
    }


    function MudarAba(aba) {
        if (aba == 'Principal') {
            $("#divHemograma").hide();
