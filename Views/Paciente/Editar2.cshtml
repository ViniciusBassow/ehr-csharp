    function RemoverExame(id, novo) {
        if (novo) {
            $("#idExameContainer" + id).remove();
            $("#idExameBtn" + id).remove();

            $("input[name='Exame[" + id + "].Medicacao']").remove();
            $("input[name='Exame[" + id + "].Dosagem']").remove();
            $("input[name='Exame[" + id + "].Intervalo']").remove();
            $("input[name='Exame[" + id + "].Duracao']").remove();
        }
        else {
            if (!confirm("Tem certeza de que deseja remover esta Medicação? \nEsta ação não poderá ser desfeita!")) return;

            var medicamentoId = $("#Exame" + id + "Id").val();

            $.ajax({
                url: '@Url.Action("ExcluirExamePrescricao", "Consulta")',
                type: 'POST',
                data: { "Id": medicamentoId },
                success: function (data) {
                    $("#idExameContainer" + id).remove();
                    $("#idExameBtn" + id).remove();

                    $("input[name='Exame[" + id + "].Medicacao']").remove();
                    $("input[name='Exame[" + id + "].Dosagem']").remove();
                    $("input[name='Exame[" + id + "].Intervalo']").remove();
                    $("input[name='Exame[" + id + "].Duracao']").remove();

                    displayMensagemSucessoAjax();
                },
                error: function (xhr, status, error) {
                    alert("Ocorreu um erro ao excluir prescrição");
                }
            });
        }
    }
